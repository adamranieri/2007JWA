public class AccountDaoImpl implements AccountDao{

private static AccountDaoImpl dao = null;
	
	private AccountDaoImpl() {
		
	};
	
	public static AccountDao getAccountDao() {
		
		if(dao == null) {
			dao = new AccountDaoImpl();
			return dao;
		}else {
			return dao;
		}
		
	}

	
